#include <stdio.h>

int main()
{
    int i,j,c=0,v,n;
    char s[1000000];
    scanf("%d",&n);
    scanf("%s",s);
    
    for(i=0;i<n;i++){
        if(s[i]=='U'){
            c++;
            if(c==0){
                v++;
            }
        }
        else{
            c--;
        }
        
    }
    printf("%d",v);
}
